class Person {
    constructor(name, age){
      this.name = name;
      this.age = age;
    }
  

  tellMyName() {
      return(`I am "${this.name}`)
  };

  tellMyAge() {
      return(`I am ${this.age} year old`)
  };

};

const personMale = new Person("John", 40)
const personFemale = new Person("Mary", 30)

personMale.tellMyName();
personMale.tellMyAge();
personFemale.tellMyName();
personFemale.tellMyAge();
